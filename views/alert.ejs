    <!--  BEGIN CONTENT AREA  -->
    <div id="content" class="main-content" style="margin-top:15px">

        <div class="layout-px-spacing">
            <blockquote class="blockquote">
                <h4>Alert List</h4>
            </blockquote>
            <div class="row layout-top-spacing">
                <div id="tableOffice" class="col-lg-12 col-12 layout-spacing">
                    <div class="statbox widget box box-shadow">
                        <div class="widget-header">
                            <div class="row">
                                <div class="col-xl-6 col-md-6 col-sm-6 col-6">
                                    <h4>Alert List</h4>
                                </div>
                                <div class="col-xl-6 col-md-6 col-sm-6 col-6" style="text-align: right">
                                    <a class="btn btn-danger mb-2 mt-2" href="/notification/wipe">Wipe ALL</a>
                                </div>
                            </div>
                        </div>
                        <div class="widget-content widget-content-area">
                            <div class="table-responsive">
                                <% if (alerts && Object.keys(alerts).length > 0) { %>
                                    <table class="table table-bordered table-hover mb-4">
                                        <thead>
                                        <tr>
                                            <th>Alert ID</th>
                                            <th>Camera Name</th>
                                            <th>Time</th>
                                            <th>Alert Message</th>
                                            <th>View</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <% alerts.forEach((alert, index) => { %>
                                            <tr>
                                                <td><%= alert.alertID %></td>
                                                <td><%= alert.cameraSN %></td>
                                                <td><%= new Date(alert.alertTime * 1000).toLocaleString("en-US") %></td>
                                                <td>
                                                        <% if (alert.alertMessage.substr(0,1) === '1') { %>Detected: No Hat<br><% } %>
                                                        <% if (alert.alertMessage.substr(0,1) === '2') { %>Detected: Wearing Hat<br><% } %>
                                                        <% if (alert.alertMessage.substr(1,1) === '1') { %>Detected: No Vest<br><% } %>
                                                        <% if (alert.alertMessage.substr(1,1) === '2') { %>Detected: Wearing Vest<br><% } %>
                                                        <% if (alert.alertMessage.substr(2,1) === '2') { %>Detected: Intrusion in Restricted Area<br><% } %>
                                                        <% if (alert.alertMessage.substr(3,1) === '2') { %>Detected: Running<br><% } %>
                                                        <% if (alert.alertMessage.substr(4,1) === '2') { %>Detected: Failing<br><% } %>
                                                </td>
                                                <td class="text-center">
                                                    <ul class="table-controls">
                                                        <li><a href="/alert/<%= alert.alertAttachment %>" target="_blank"
                                                               data-toggle="tooltip"
                                                               data-placement="top" title=""
                                                               data-original-title="View Alert Image">
                                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-video">
                                                                    <polygon points="23 7 16 12 23 17 23 7"></polygon>
                                                                    <rect x="1" y="5" width="15" height="14" rx="2" ry="2"></rect>
                                                                </svg>
                                                            </a></li>
                                                    </ul>
                                                </td>
                                            </tr>
                                        <% }) %>
                                        </tbody>
                                    </table>
                                <% } else { %>
                                    <div class="alert alert-danger mb-4" role="alert">
                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                                 viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                                 stroke-linecap="round" stroke-linejoin="round"
                                                 class="feather feather-x close" data-dismiss="alert">
                                                <line x1="18" y1="6" x2="6" y2="18"></line>
                                                <line x1="6" y1="6" x2="18" y2="18"></line>
                                            </svg>
                                        </button>
                                        <strong>Error!</strong> No Alert Found.
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
